{
  "$schema": "vscode://schemas/color-theme",
  "name": "Barva Dark",
  "type": "dark",
  "colors": {
    "activityBar.background": "#2D3532",
    "activityBar.foreground": "#E0E5E0",
    "activityBar.inactiveForeground": "#7F8C8D",
    "activityBar.border": "#3A423F",
    "editor.background": "#252A27",
    "editor.foreground": "#E0E5E0",
    "editor.lineHighlightBackground": "#7A9AD422",
    "editor.selectionBackground": "#7A9AD455",
    "editor.selectionHighlightBackground": "#7A9AD433",
    "editorCursor.foreground": "#7A9AD4",
    "editorCursor.background": "#7A9AD422",
    "editorLineNumber.foreground": "#6B7280",
    "editorLineNumber.activeForeground": "#E0E5E0",
    "editorError.foreground": "#E55B5B",
    "editorWarning.foreground": "#FFD107",
    "editorBracketMatch.background": "#7A9AD433",
    "editorBracketMatch.border": "#7A9AD4",
    "editor.wordHighlightBackground": "#6AA66A33",
    "editor.wordHighlightStrongBackground": "#7A9AD433",
    "editor.findMatchBackground": "#F0C10766",
    "editor.findMatchHighlightBackground": "#F0C10733",
    "diffEditor.insertedTextBackground": "#6AA66A33",
    "diffEditor.removedTextBackground": "#E55B5B33",
    "statusBar.background": "#2D3532",
    "statusBar.foreground": "#E0E5E0",
    "statusBar.noFolderBackground": "#3A423F",
    "statusBar.debuggingBackground": "#E55B5B",
    "statusBarItem.errorBackground": "#E55B5B33",
    "statusBarItem.warningBackground": "#FFD10733",
    "tab.activeBackground": "#3A423F",
    "tab.activeForeground": "#E0E5E0",
    "tab.inactiveBackground": "#2D3532",
    "tab.inactiveForeground": "#7F8C8D",
    "tab.hoverBackground": "#3A423F",
    "tab.activeBorder": "#7A9AD4",
    "tab.activeBorderTop": "#7A9AD4",
    "sideBar.background": "#2D3532",
    "sideBar.foreground": "#E0E5E0",
    "sideBar.border": "#3A423F",
    "sideBarSectionHeader.background": "#3A423F",
    "list.activeSelectionBackground": "#3A423F",
    "list.hoverBackground": "#3A423F66",
    "list.focusBackground": "#7A9AD422",
    "list.highlightForeground": "#7A9AD4",
    "input.background": "#2D3532",
    "input.border": "#3A423F",
    "inputValidation.errorBorder": "#E55B5B",
    "inputValidation.warningBorder": "#FFD107",
    "inputValidation.infoBorder": "#7A9AD4",
    "scrollbarSlider.background": "#7A9AD433",
    "scrollbarSlider.hoverBackground": "#7A9AD466",
    "scrollbarSlider.activeBackground": "#7A9AD499",
    "terminal.background": "#2D3532",
    "terminal.foreground": "#E0E5E0",
    "terminal.ansiBlack": "#252A27",
    "terminal.ansiBlue": "#7A9AD4",
    "terminal.ansiBrightBlue": "#8AB4F8",
    "terminal.ansiBrightGreen": "#7BC07B",
    "terminal.ansiBrightRed": "#FF6B6B",
    "terminal.ansiBrightYellow": "#FFD107",
    "terminal.ansiGreen": "#6AA66A",
    "terminal.ansiRed": "#E55B5B",
    "terminal.ansiYellow": "#FFD107",
    "terminalCursor.foreground": "#7A9AD4",
    "widget.shadow": "#00000088",
    "focusBorder": "#7A9AD4",
    "gitDecoration.addedResourceForeground": "#6AA66A",
    "gitDecoration.modifiedResourceForeground": "#FFD107",
    "gitDecoration.deletedResourceForeground": "#E55B5B",
    "gitDecoration.untrackedResourceForeground": "#7BC07B"
  },
  "tokenColors": [
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#7A9AD4",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["string", "string.quoted", "string.regexp"],
      "settings": {
        "foreground": "#7BC07B"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language.boolean"],
      "settings": {
        "foreground": "#FFD107"
      }
    },
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#7F8C8D",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#E55B5B"
      }
    },
    {
      "scope": ["entity.name.type", "support.class", "support.type"],
      "settings": {
        "foreground": "#7A9AD4"
      }
    },
    {
      "scope": ["variable", "variable.parameter"],
      "settings": {
        "foreground": "#E0E5E0"
      }
    },
    {
      "scope": ["entity.name.tag", "meta.tag.html"],
      "settings": {
        "foreground": "#7A9AD4",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#FFD107"
      }
    },
    {
      "scope": ["punctuation", "meta.delimiter"],
      "settings": {
        "foreground": "#7A9AD4"
      }
    }
  ],
  "semanticTokenColors": {
    "variable": "#E0E5E0",
    "parameter": "#FFD107",
    "function": "#E55B5B",
    "class": "#7A9AD4",
    "interface": "#7A9AD4",
    "enum": "#7A9AD4",
    "string": "#7BC07B",
    "number": "#FFD107",
    "keyword": "#7A9AD4",
    "comment": {
      "foreground": "#7F8C8D",
      "fontStyle": "italic"
    }
  }
}
