{
  "$schema": "vscode://schemas/color-theme",
  "name": "Barva Light",
  "type": "light",
  "colors": {
    "activityBar.background": "#F0E9DF",
    "activityBar.foreground": "#2A2F2C",
    "activityBar.inactiveForeground": "#7F8C8D",
    "activityBar.border": "#D5D0C8",
    "editor.background": "#FAF5ED",
    "editor.foreground": "#2A2F2C",
    "editor.lineHighlightBackground": "#3A6F9F22",
    "editor.selectionBackground": "#3A6F9F33",
    "editor.selectionHighlightBackground": "#3A6F9F22",
    "editorCursor.foreground": "#3A6F9F",
    "editorCursor.background": "#3A6F9F22",
    "editorLineNumber.foreground": "#A3A9A6",
    "editorLineNumber.activeForeground": "#2A2F2C",
    "editorError.foreground": "#C53030",
    "editorWarning.foreground": "#B38B59",
    "editorBracketMatch.background": "#3A6F9F22",
    "editorBracketMatch.border": "#3A6F9F",
    "editor.wordHighlightBackground": "#5B8C5A22",
    "editor.wordHighlightStrongBackground": "#3A6F9F22",
    "editor.findMatchBackground": "#B38B5966",
    "editor.findMatchHighlightBackground": "#B38B5933",
    "diffEditor.insertedTextBackground": "#5B8C5A33",
    "diffEditor.removedTextBackground": "#C5303033",
    "statusBar.background": "#F0E9DF",
    "statusBar.foreground": "#2A2F2C",
    "statusBar.noFolderBackground": "#E5DFD5",
    "statusBar.debuggingBackground": "#C53030",
    "statusBarItem.errorBackground": "#C5303022",
    "statusBarItem.warningBackground": "#B38B5922",
    "tab.activeBackground": "#F0E9DF",
    "tab.activeForeground": "#2A2F2C",
    "tab.inactiveBackground": "#FAF5ED",
    "tab.inactiveForeground": "#7F8C8D",
    "tab.hoverBackground": "#E5DFD5",
    "tab.activeBorder": "#3A6F9F",
    "tab.activeBorderTop": "#3A6F9F",
    "sideBar.background": "#F0E9DF",
    "sideBar.foreground": "#2A2F2C",
    "sideBar.border": "#D5D0C8",
    "sideBarSectionHeader.background": "#E5DFD5",
    "list.activeSelectionBackground": "#D5D0C8",
    "list.activeSelectionForeground": "#2A2F2C",
    "list.focusBackground": "#D5D0C8",
    "list.focusForeground": "#2A2F2C",
    "list.hoverBackground": "#D5D0C866",
    "list.hoverForeground": "#2A2F2C",
    "list.inactiveSelectionBackground": "#E5DFD5",
    "list.inactiveSelectionForeground": "#2A2F2C",
    "input.background": "#FAF5ED",
    "input.border": "#D5D0C8",
    "inputValidation.errorBorder": "#C53030",
    "inputValidation.warningBorder": "#B38B59",
    "inputValidation.infoBorder": "#3A6F9F",
    "scrollbarSlider.background": "#3A6F9F33",
    "scrollbarSlider.hoverBackground": "#3A6F9F55",
    "scrollbarSlider.activeBackground": "#3A6F9F77",
    "terminal.background": "#F0E9DF",
    "terminal.foreground": "#2A2F2C",
    "terminal.ansiBlack": "#2A2F2C",
    "terminal.ansiBlue": "#3A6F9F",
    "terminal.ansiBrightBlue": "#5B8BBF",
    "terminal.ansiBrightGreen": "#5B8C5A",
    "terminal.ansiBrightRed": "#C53030",
    "terminal.ansiBrightYellow": "#B38B59",
    "terminal.ansiGreen": "#5B8C5A",
    "terminal.ansiRed": "#C53030",
    "terminal.ansiYellow": "#B38B59",
    "terminalCursor.foreground": "#3A6F9F",
    "widget.shadow": "#00000022",
    "focusBorder": "#3A6F9F",
    "gitDecoration.addedResourceForeground": "#5B8C5A",
    "gitDecoration.modifiedResourceForeground": "#B38B59",
    "gitDecoration.deletedResourceForeground": "#C53030",
    "gitDecoration.untrackedResourceForeground": "#7BC07B",
    "menu.background": "#F0E9DF",
    "menu.foreground": "#2A2F2C",
    "menu.selectionBackground": "#D5D0C8",
    "menu.selectionForeground": "#2A2F2C",
    "menu.selectionBorder": "#3A6F9F",
    "menu.separatorBackground": "#D5D0C8"
  },
  "tokenColors": [
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#3A6F9F",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["string", "string.quoted", "string.regexp"],
      "settings": {
        "foreground": "#5B8C5A"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language.boolean"],
      "settings": {
        "foreground": "#B38B59"
      }
    },
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#A3A9A6",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#C53030"
      }
    },
    {
      "scope": ["entity.name.type", "support.class", "support.type"],
      "settings": {
        "foreground": "#3A6F9F"
      }
    },
    {
      "scope": ["variable", "variable.parameter"],
      "settings": {
        "foreground": "#2A2F2C"
      }
    },
    {
      "scope": ["entity.name.tag", "meta.tag.html"],
      "settings": {
        "foreground": "#3A6F9F",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#B38B59"
      }
    },
    {
      "scope": ["punctuation", "meta.delimiter"],
      "settings": {
        "foreground": "#3A6F9F"
      }
    }
  ],
  "semanticTokenColors": {
    "variable": "#2A2F2C",
    "parameter": "#B38B59",
    "function": "#C53030",
    "class": "#3A6F9F",
    "interface": "#3A6F9F",
    "enum": "#3A6F9F",
    "string": "#5B8C5A",
    "number": "#B38B59",
    "keyword": "#3A6F9F",
    "comment": {
      "foreground": "#A3A9A6",
      "fontStyle": "italic"
    }
  }
}
